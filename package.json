{
  "name": "@bigcommerce/bigpay-client",
  "version": "5.7.0",
  "description": "JavaScript client for Bigpay",
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "not Baidu > 0",
    "not QQAndroid > 0",
    "not Android < 62"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib/",
    "src"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "lint": "eslint src test",
    "prerelease": "npm run lint && npm test",
    "release": "standard-version -a",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bigcommerce/bigpay-client-js.git"
  },
  "author": "BigCommerce",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.1.3",
    "gulp-conventional-changelog": "^2.0.29",
    "gulp-git": "^2.10.0",
    "gulp-prompt": "^1.2.0",
    "gulp-touch": "^1.0.1",
    "jest": "^25.2.0",
    "lodash": "^4.17.15",
    "semver": "^7.1.3",
    "standard-version": "^7.1.0",
    "xhr-mock": "^2.5.1"
  },
  "dependencies": {
    "@bigcommerce/form-poster": "^1.4.0",
    "babel-jest": "^25.2.0",
    "deep-assign": "^3.0.0",
    "object-assign": "^4.1.1"
  }
}
